{
  "manifest_version": 2,

  "name": "Soran",
  "description": "소란!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "background": {
    "page": "background.html"
  },

  "permissions": [
    "cookies",
    "http://bugs.co.kr/*",
    "http://127.0.0.1:9001/*", "http://127.0.0.1:9000/*"
  ],

  "content_scripts": [
    {
      "matches": [
        "http://music.naver.com/*",
        "http://player.music.naver.com/*",
        "http://music.bugs.co.kr/*",
        "http://bugs.co.kr/*",
        "http://*.bugs.co.kr/*"
      ],
      "js": ["jquery.js","contentScript.js"],
      "run_at": "document_end"
    }
  ]
}
