{
  "manifest_version": 2,

  "name": "Soran",
  "description": "소란!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "background": {
    "page": "background.html"
  },

  "permissions": ["http://192.168.0.174:9001/*"],

  "content_scripts": [
    {
      "matches": [
        "http://www.bugs.co.kr/*",
        "http://music.naver.com/*",
        "http://*.bugs.co.kr/*"
      ],
      "js": ["jquery.js", "contentScript.js"]
    }
  ]
}
